# Environment variables for local development
# Copy this file to .env and update with your actual values

# Frontend domain (e.g., https://api.example.com or http://localhost:3000)
FRONTEND_DOMAIN=https://www.example.com

# Backend/API domain (e.g., https://api.example.com or http://localhost:3001)
# API_URL will be automatically derived from BACKEND_DOMAIN if not explicitly set
BACKEND_DOMAIN=https://api.example.com

# Data directory (optional, defaults to ./data)
# DATA_DIR=./data
