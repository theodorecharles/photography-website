{
  "environment": {
    "frontend": {
      "port": 3000,
      "apiUrl": "http://localhost:3001"
    },
    "backend": {
      "port": 3001,
      "allowedOrigins": ["http://localhost:3000"]
    },
    "optimization": {
      "concurrency": 4,
      "images": {
        "thumbnail": {
          "quality": 60,
          "maxDimension": 640
        },
        "modal": {
          "quality": 90,
          "maxDimension": 2048
        },
        "download": {
          "quality": 100,
          "maxDimension": 4096
        }
      },
      "video": {
        "segmentDuration": 4,
        "resolutions": {
          "240p": {
            "enabled": true,
            "height": 240,
            "videoBitrate": "400k",
            "audioBitrate": "128k"
          },
          "360p": {
            "enabled": true,
            "height": 360,
            "videoBitrate": "800k",
            "audioBitrate": "128k"
          },
          "720p": {
            "enabled": true,
            "height": 720,
            "videoBitrate": "2500k",
            "audioBitrate": "192k"
          },
          "1080p": {
            "enabled": true,
            "height": 1080,
            "videoBitrate": "5000k",
            "audioBitrate": "192k"
          }
        }
      }
    },
    "security": {
      "allowedHosts": ["localhost:3000"],
      "rateLimitWindowMs": 1000,
      "rateLimitMaxRequests": 100,
      "redirectFrom": [],
      "redirectTo": ""
    },
    "logging": {
      "level": "error"
    },
    "auth": {
      "google": {
        "clientId": "your-google-client-id",
        "clientSecret": "your-google-client-secret"
      },
      "sessionSecret": "your-session-secret-here",
      "authorizedEmails": ["your-email@example.com"]
    }
  },
  "branding": {
    "siteName": "Your Name",
    "avatarPath": "/photos/avatar.png",
    "primaryColor": "#4ade80",
    "secondaryColor": "#22c55e",
    "metaDescription": "Photography portfolio by Your Name",
    "metaKeywords": "photography, portfolio, your name",
    "faviconPath": "/favicon.ico",
    "language": "en"
  },
  "analytics": {
    "scriptPath": "",
    "openobserve": {
      "enabled": false,
      "endpoint": "https://log.yourdomain.com/api/",
      "organization": "your-organization-id",
      "stream": "website",
      "username": "your-username",
      "password": "your-password"
    },
    "hmacSecret": ""
  },
  "externalLinks": [
    {
      "title": "My Blog",
      "url": "https://blog.example.com"
    },
    {
      "title": "Primes Benchmark",
      "url": "/primes/"
    },
    {
      "title": "YouTube",
      "url": "https://youtube.com/@yourname"
    },
    {
      "title": "GitHub",
      "url": "https://github.com/yourusername/"
    },
    {
      "title": "Login",
      "url": "/admin"
    }
  ],
  "openai": {
    "apiKey": ""
  },
  "ai": {
    "autoGenerateTitlesOnUpload": false
  },
  "pushNotifications": {
    "enabled": false,
    "vapidPublicKey": "",
    "vapidPrivateKey": "",
    "vapidSubject": "mailto:admin@example.com"
  }
}
